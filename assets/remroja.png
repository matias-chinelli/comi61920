                    $('#iframe-' + modalID[$(this)[0].id]).attr('src', url[$(this)[0].id]);
                    $('#' + modalID[$(this)[0].id]).appendTo("body");
                })
            }

            length = modals.length;

            for (i = 0; i < length; i++) {
                modalID[modals[i].id] = modals[i].id;
                $('#' + modalID[modals[i].id]).on('shown.bs.modal', function () {
                    $('#' + modalID[$(this)[0].id]).appendTo("body");
                })
            }

        });

</script>



